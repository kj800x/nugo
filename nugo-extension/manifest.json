{
  "name": "NUgo",
  "version": "0.0.1",
  "description": "Quick links to Northeastern University sites for students!",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background/flagExtension.js",
      "nugo-common/data.js",
      "nugo-common/searchText.js",
      "background/registerHandlers.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
      "data/urls.json"
  ],
  "omnibox": { "keyword" : "go" },
  "browser_action": {
    "default_title": "Open NUgo"
  },
  "manifest_version": 2
}
