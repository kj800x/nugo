{
  "name": "NUgo",
  "version": "0.1.3",
  "description": "Never need to use MyNEU again!",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background/flagExtension.js",
      "nugo-common/data.js",
      "nugo-common/searchText.js",
      "background/registerHandlers.js"
    ],
    "persistent": false
  },
  "omnibox": { "keyword" : "go" },
  "browser_action": {
    "default_title": "Open NUgo"
  },
  "manifest_version": 2
}
